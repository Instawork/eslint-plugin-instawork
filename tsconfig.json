{
    "compilerOptions": {
      "target": "esnext",
      "module": "commonjs",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noImplicitAny": true,
      "strictNullChecks": true,
      "noImplicitReturns": true,
    },
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Recommended",
    "include": ["src/**/*"]
}
