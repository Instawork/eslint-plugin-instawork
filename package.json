{
  "name": "eslint-plugin-instawork",
  "version": "0.3.2-4",
  "main": "lib/index.js",
  "description": "ESLint plugin with opinionated rules used at Instawork",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir lib",
    "prettify": "pretty-quick --staged",
    "release:prerelease": "npm version prerelease && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "test": "eslint src --max-warnings 0 && jest src"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "3.2.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-flowtype": "3.9.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.13.0",
    "humps": "2.0.1"
  },
  "peerDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/cli": "7.4.4",
    "@babel/preset-env": "7.4.5",
    "@babel/runtime": "7.4.5",
    "babel-eslint": "10.0.1",
    "eslint": "5.16.0",
    "jest": "24.8.0",
    "eslint-plugin-instawork": "file:./src",
    "prettier": "1.17.1",
    "pretty-quick": "1.11.0"
  }
}
