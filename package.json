{
  "name": "eslint-plugin-instawork",
  "version": "0.6.1",
  "main": "src/index.js",
  "description": "ESLint plugin with opinionated rules used at Instawork",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Instawork/eslint-plugin-instawork"
  },
  "license": "MIT",
  "scripts": {
    "build": "babel src --out-dir lib",
    "prettify": "pretty-quick --staged",
    "release:prerelease": "yarn version prerelease && yarn publish && git push --follow-tags",
    "release:patch": "yarn version patch && yarn publish && git push --follow-tags",
    "release:minor": "yarn version minor && yarn publish && git push --follow-tags",
    "release:major": "yarn version major && yarn publish && git push --follow-tags",
    "test": "eslint src --max-warnings 0 && jest src"
  },
  "dependencies": {
    "babel-plugin-module-resolver": "4.1.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.0.0",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-plugin-flowtype": "5.3.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "humps": "2.0.1"
  },
  "peerDependencies": {
    "@babel/eslint-parser": "7.13.4",
    "eslint": "7.20.0",
    "react": "17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.13.0",
    "@babel/core": "7.13.1",
    "@babel/eslint-parser": "7.13.4",
    "@babel/preset-env": "7.13.5",
    "@babel/runtime": "7.13.6",
    "babel-eslint": "10.1.0",
    "eslint": "7.20.0",
    "eslint-plugin-instawork": "file:.",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react": "17.0.1"
  }
}
