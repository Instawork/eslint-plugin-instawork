{
  "extends": "plugin:instawork/recommended",
  "plugins": [ "instawork" ],
  "env": {
    "jest": true,
    "node": true
  },
  "rules": {
    "instawork/error-object": 0,
    "global-require": 0
  },
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {
        "instawork/flow-annotate": "off"
      }
    }
  ]
}
